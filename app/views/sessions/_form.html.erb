<div class="form-group">
  <%= form_tag sessions_path do %>
    <form role="form">
      <%= label_tag :email %>
      <%= text_field_tag :email, "", :class => "form-control" %>
      <%= label_tag :password %>
      <%= password_field_tag :password, "", :class => "form-control" %>
    </div>
      <%= submit_tag "Log In", :class => "btn btn-default" %>
    </form>
  <% end %>
